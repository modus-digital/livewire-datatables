{
    "preset": "laravel",
    "rules": {
        "blank_line_before_statement": {
            "statements": [
                "return",
                "try"
            ]
        },
        "concat_space": {
            "spacing": "one"
        },
        "not_operator_with_successor_space": true,
        "logical_operators": true
    },
    "exclude": [
        "node_modules",
        "vendor"
    ]
}
